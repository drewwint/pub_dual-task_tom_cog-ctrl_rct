        <Param val="" valType="extendedCode" updates="constant" name="End Experiment"/>
        <Param val="" valType="extendedCode" updates="constant" name="End JS Experiment"/>
        <Param val="psychoJS.experiment.addData(&quot;trial_type&quot;, trial_type);&amp;#10;psychoJS.experiment.addData(&quot;ssd&quot;, ssd);&amp;#10;psychoJS.experiment.addData(&quot;ssst_resp&quot;, ssst_resp.keys);&amp;#10;if ((trial_type === &quot;stop&quot;)) {&amp;#10;    if ((ssst_resp.corr === 1)) {&amp;#10;        ssd += stepsize;&amp;#10;    } else {&amp;#10;        ssd -= stepsize;&amp;#10;    }&amp;#10;    if ((ssd &lt; minval)) {&amp;#10;        ssd = minval;&amp;#10;    }&amp;#10;    if ((ssd &gt; maxval)) {&amp;#10;        ssd = maxval;&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="End JS Routine"/>
        <Param val="thisExp.addData('trial_type', trial_type)&amp;#10;thisExp.addData('ssd', ssd)&amp;#10;thisExp.addData('ssst_resp', ssst_resp.keys)&amp;#10;&amp;#10;if trial_type == 'stop': # only adjust on stop trials&amp;#10;    #adjust the stepsize&amp;#10;    if ssst_resp.corr == 1:&amp;#10;        ssd += stepsize&amp;#10;    else:&amp;#10;        ssd -= stepsize&amp;#10;&amp;#10;    # cap the stop time&amp;#10;    if ssd &lt; minval:&amp;#10;        ssd = minval&amp;#10;    if ssd &gt; maxval:&amp;#10;        ssd = maxval&amp;#10;&amp;#10;#print(ssd)" valType="extendedCode" updates="constant" name="End Routine"/>
